    "svg-url-loader": "^5.0.0",
    "terser-webpack-plugin": "^3.0.1",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "wdio-chromedriver-service": "^6.0.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-pwa-manifest": "^4.2.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
    "app/**/*.{js,jsx}": [
      "npm run lint:js:fix",
      "git add --force"
    ],
    "app/**/*.json": [
      "prettier --write",
      "git add --force"
    ],
    "app/**/*.less": [
      "npm run lint:style:fix",
      "git add --force"
    ],
    "app/**/*.{ts,tsx}": [
      "npm run lint:tslint",
      "git add --force"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dteenergy/wismo-ui"
  },
  "scripts": {
    "build": "npm run lint && npm run bundle",
    "build:dev": "npm run lint && npm run bundle:dev",
    "bundle": "cross-env NODE_ENV=production webpack --color --config internals/webpack/webpack.prod.js --display-optimization-bailout --hide-modules -p --progress",
    "bundle:dev": "cross-env NODE_ENV=dev webpack --color  --config internals/webpack/webpack.dev.js --display-optimization-bailout --hide-modules -p --progress",
    "docker:build": "npm run build:dev && docker build -t wismoui .",
    "docker:list": "docker ps -a",
    "docker:remove": "docker container rm",
    "docker:start:mac": "internals/docker/launch-docker.sh",
    "docker:start": "docker run --name=wismoui -v %cd%/:/app -p 81:8181 wismoui",
    "docker:stop": "docker container stop",
    "docker:stop:mac": "docker rm $(docker ps -a | awk 'NR>1' | grep wismoui | awk '{print $1}' | xargs)",
    "e2e": "wdio run ./wdio.conf.js",
    "e2e:edge": "npm run e2e -- --edge",
    "e2e:edge:headless": "npm run e2e -- --edge --headless",
    "e2e:firefox": "npm run e2e -- --firefox",
    "e2e:firefox:headless": "npm run e2e -- --firefox --headless",
    "e2e:headless": "npm run e2e -- --headless",
    "extract-messages": "ts-node internals/scripts/extract-messages.ts",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:style && npm run lint:ts",
    "lint:css": "stylelint app/**/*.tsx",
    "lint:fix": "npm run lint:js:fix",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts -- . ",
    "lint:js:fix": "eslint --fix --ignore-path .gitignore .",
    "lint:style": "stylelint app/**/*.less --syntax less",
    "lint:style:fix": "stylelint app/**/*.less --syntax less --fix",
    "lint:ts": "npm run lint:tslint",
    "lint:tslint": "tslint --config ./tslint.json --project ./tsconfig.json",
    "prettify": "prettier --write",
    "start": "npm run build:dev && npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev webpack-dev-server --config internals/webpack/webpack.dev.js",
    "test": "cross-env NODE_ENV=test jest --coverage --no-cache",
    "test:quick": "cross-env NODE_ENV=test jest --coverage=false --no-cache",
    "test:update": "cross-env NODE_ENV=test jest --coverage --updateSnapshot",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  }
}
