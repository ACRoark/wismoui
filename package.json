    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@wdio/allure-reporter": "^6.10.6",
    "@wdio/cli": "^6.10.5",
    "@wdio/cucumber-framework": "^6.10.6",
    "@wdio/local-runner": "^6.10.5",
